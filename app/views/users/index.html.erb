<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>&nbsp;
          <%= current_user_friend(user) %>
          <%= pending1(user) %>
          <%= pending(user) %>
          <%= friend_request1(user) %>
          <%= friend_request2(user) %>
          <%= friend_request3(user) %>
          <%= add_friend(user) %>
        </span>
      </li>
    <% end %>
  </ul>
</section>